class Solution {
    public int rob(int[] nums) {
        int n = nums.length ;
        if(n == 1){
            return nums[0] ;
        }
        int [] M = new int[n] ;
        M[0] = nums[0] ;
        M[1] = Math.max(nums[0] , nums[1]);
        for(int i = 2 ; i<n ; i++){
            M[i] = Math.max(nums[i] + M[i-2] , M[i-1] ) ;
        }
        return M[n-1] ;
    }
}
